page {
  background-color: #F3F3F3;
}

.runningWaterItem {
  padding: 10rpx 0 10rpx;
}

.userItem {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx;
  background-color: white;
  border-bottom: 1px solid #F3F3F3;
  margin-top: 20rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-radius: 15rpx;
}

.item {
  display: flex;
  flex-direction: column;
}

.itemTitle {
  font-size: 28rpx;
  color: #333333;
}

.itemNote {
  font-size: 22rpx;
  color: #999999;
  margin-top: 10rpx;
}